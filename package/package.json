{
  "name": "djuno-design",
  "version": "1.2.1",
  "description": "Djuno design lib for React projects",
  "keywords": [
    "djuno",
    "design",
    "react"
  ],
  "scripts": {
    "start": "nodemon --watch src --ext ts,tsx,js,jsx,css,mjs --exec npm run build",
    "build": "npm run prettier && npm run rollup && npm run link",
    "link": "npm link",
    "rollup": "rollup -c",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write \"src/**/*.{js,ts,jsx,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Djuno-Ltd/djuno-design"
  },
  "author": "Djuno Ltd (Ansar Mirzayi) <ansar@djuno.io>",
  "license": "Apache-2.0",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "directories": {
    "src": "src"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@floating-ui/react-dom": "^2.1.1",
    "@headlessui/react": "^2.1.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/rollup": "^8.1.0",
    "@types/react": "^18.3.18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@uiw/react-json-view": "^2.0.0-alpha.27",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^11.3.29",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.35",
    "postcss-prefixer": "^3.0.0",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.26.3",
    "rollup": "^4.9.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-import-css": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2"
  },
  "peerDependencies": {
    "react": "^18.x",
    "react-dom": "^18.x"
  }
}
